                    <nav class="navbar topnavbar">
                        <!-- START navbar header-->
                        <div class="navbar-header">
                            <a class="navbar-brand" href="#/">
                                <div class="brand-logo">
                                    @{

                                        string logo = string.Empty;
                                        if (string.IsNullOrEmpty(Convert.ToString(Session["Logo"])))
                                        {
                                            logo = "~/Content/Template/img/logo-single.png";
                                        }
                                        else
                                        {
                                            logo = Convert.ToString(Session["Logo"]);
                                        }



                                    }
                                    <img class="img-fluid" src="@Url.Content(logo)" alt="App Logo" height="30" width="30">
                                </div>
                                <div class="brand-logo-collapsed">
                                    <img class="img-fluid" src="@Url.Content(logo)" alt="App Logo" height="30" width="30">
                                </div>
                            </a>
                        </div>
